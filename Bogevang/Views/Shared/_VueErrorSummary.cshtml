<div v-if="errors.length" v-cloak>
  <div v-for="error in errors" class="alert alert-danger" role="alert">{{ error }}</div>
</div>

<div v-if="notifications.length" v-cloak>
  <div v-for="notification in notifications" :class="notificationAlertClass(notification.level)" role="alert">{{ notification.message }}</div>
</div>
